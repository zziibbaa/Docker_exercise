name: Deploy_Model_project
conda_env : conda.yaml

entry_points:
  main:
    parameters:
      filename: {type: str}
      n_estimators: {type: int, default: 50}
      random_state: {type: int, default: 101}
    command: >
      python Deploy_Model.py --filename {filename} --n_estimators {n_estimators} --random_state {random_state}
